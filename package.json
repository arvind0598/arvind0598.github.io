{
  "name": "resume",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf .cache",
    "start": "npm run clean && parcel ./src/*.pug",
    "prebuild": "npm run clean && parcel build ./src/*.pug --no-source-maps --experimental-scope-hoisting --no-content-hash",
    "build": "purgecss -c ./purgecss.config.js -o ./dist/",
    "postbuild": "touch dist/CNAME && echo \"www.arvindsuresh.in\" > dist/CNAME",
    "lint": "eslint ./src --ext .js",
    "lintfix": "eslint ./src --ext .js --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bulma": "^0.7.5",
    "chart.js": "^2.8.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.4",
    "pug-html-loader": "^1.1.5",
    "purgecss": "^1.3.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1"
  }
}
